{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "preserve",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"strict": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	}
}
